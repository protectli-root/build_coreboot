# layout for firmware residing at top of 4GB address space
# +-------------+ <-- 4GB - ROM_SIZE / start of flash
# | unspecified |
# +-------------+ <-- 4GB - BIOS_SIZE
# | FMAP        |
# +-------------+ <-- 4GB - BIOS_SIZE + FMAP_SIZE
# | CBFS        |
# +-------------+ <-- 4GB / end of flash

FLASH@4294705152 0x00040000 {
	BIOS@0 262144 {
		
		
		
		
		
		FMAP@0 0x200
		COREBOOT(CBFS)@512 261632
	}
}
